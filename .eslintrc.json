{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "func-names": "off",
    "no-alert": "off",
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "prefer-const": "off"
  }
}
